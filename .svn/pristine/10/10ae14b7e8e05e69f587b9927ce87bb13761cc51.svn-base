    <script>
    $(function($){
    		
	    	setTimeout(time, 1000);
		    function time () {
		    	var show_online_num =Math.round(Math.random()*2000);
		    	var register_count =Math.round(Math.random()*200 + 500);
		    	var offers_rpize =Math.random()*1000000 + 20000000;
		    	var active_deposit =Math.round(Math.random()*50);
		    	var active_deposit_min =Math.round(Math.random()*1);
		    	var active_awal_minute =Math.round(Math.random());
		    	var active_awal_second =Math.round(Math.random()*150);

		    	var default_data = {
	        			"show_online_num":show_online_num,
	        			"register_count":register_count,
	        			"offers_rpize":offers_rpize.toFixed(2),
	        			"active_deposit":active_deposit,
	        			"active_deposit_min":active_deposit_min,
	        			"active_awal_minute":active_awal_minute,
	        			"active_awal_second":active_awal_second
		        	};
		        	
		            $("#show_online_num_1").html(default_data.show_online_num);
		            $("#show_online_num_2").html(default_data.show_online_num);
		            
		            $("#offers_rpize").html(default_data.offers_rpize);
		        	$("#register_count").html(default_data.register_count);
		    		$("#minutes").html("0" + default_data.active_deposit_min);
		    		$("#seconds").html(default_data.active_deposit);
		    		$("#minutes2").html(default_data.active_awal_minute);
		    		$("#seconds2").html(default_data.active_awal_second);

				    //TIMER
				    var minutesLabel = document.getElementById("minutes");
				    var secondsLabel = document.getElementById("seconds");

				    var minutesLabel2 = document.getElementById("minutes2");
				    var secondsLabel2 = document.getElementById("seconds2");

				    var totalSeconds1 = 0;
				    var totalSeconds2 = 0;
				    var total1 = parseInt(default_data.active_deposit_min*60) + parseInt(default_data.active_deposit);
				    var total2 = parseInt(default_data.active_awal_minute*60) + parseInt(default_data.active_awal_second);

				    var setTime1 = setInterval(setTime1, 80);
				    var setTime2 = setInterval(setTime2, 5);

				    function setTime1() {

				        ++totalSeconds1;

				        if (totalSeconds1 <= total1) {
				            secondsLabel.innerHTML = pad(totalSeconds1 % 60);
				            minutesLabel.innerHTML = pad(parseInt(totalSeconds1 / 60));
				        }

				    }

				    function setTime2() {
				        ++totalSeconds2;

				        if (totalSeconds2 <= total2) {
				            secondsLabel2.innerHTML = pad(totalSeconds2 % 60);
				            minutesLabel2.innerHTML = pad(parseInt(totalSeconds2 / 60));
				        } else {
				            clearInterval(setTime2);
				        }
				    }

				    function pad(val) {
				        var valString = val + "";
				        if (valString.length < 2) {
				            return "0" + valString;
				        }
				        else {
				            return valString;
				        }
				    }
		    }
        $(function(){
            Carousel.init($("#carousel"));
            $("#carousel").init();
        });
		    
    });

		</script>
