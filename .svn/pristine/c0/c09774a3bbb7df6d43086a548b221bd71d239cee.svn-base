	position: fixed;
	z-index: 999999
}

.clear {
	clear: both
}

.loading-wrap {
	width: 102px;
	height: 102px;
	position: fixed;
	top: 30%;
	left: 50%;
	margin-left: -51px;
	background: url(http://192.168.0.140:81/TXWN/user_img/loading-logo.png) no-repeat;
	z-index: 9999
}

.loading-point {
	position: absolute;
	left: -10px;
	top: -10px;
	z-index: 1;
	width: 122px;
	height: 122px;
	background-repeat: no-repeat;
	background-image: url(http://192.168.0.140:81/TXWN/user_img/loading-sprite.png);
	background-position: 0 0
}

.loading-point.animated {
	-webkit-animation: circle 2s linear infinite forwards;
	animation: circle 2s linear infinite forwards
}

@-webkit-keyframes circle {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg)
	}
	100% {
		-webkit-transform: rotate(360deg);
		transform: rotate(360deg)
	}
}

@keyframes circle {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg)
	}
	100% {
		-webkit-transform: rotate(360deg);
		transform: rotate(360deg)
	}
}

