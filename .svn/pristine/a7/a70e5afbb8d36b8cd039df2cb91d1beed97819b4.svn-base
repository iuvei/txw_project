ul.tabs {
    list-style-type: none;
    margin: 0;
    padding: 0 0 0 100px;
    /*width: 880px;*/
    z-index: 10;
}
ul.tabs li.active {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #CCCCCC #CCCCCC #FFFFFF;
    border-image: none;
    border-radius: 3px 3px 0 0;
    border-style: solid;
    border-width: 1px;
    margin-bottom: -1px;
    z-index: 11;
}
ul.tabs li {
    display: inline-block;
    /*margin: 0 -4px 0 20px;*/
    padding: 10px 0;
    text-align: center;
    /*width: 100px;*/
    width: 120px;
}
ul.tabs li.active{ color: #9973e6; font-size: 18px; height: 20px; margin-top: 1px; background: #fff;}
ul.tabs li.active a {
    color: #9973e6;
}
ul.tabs li a {
    color: #544771;
    font-size: 18px;
}

