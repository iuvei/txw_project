.email_buttons  .btn_email1 {cursor:pointer; font-size:18px; display:inline-block; background:#9973e6; width:260px; height:44px; line-height:44px; text-align:center; color:#fff; border-radius:5px; }

.settings_main { width:272px; height:54px; background:#2e2e2e; line-height:54px; text-align:center; color:#fff; border-radius:3px; display:block; margin:200px auto 0; box-shadow: 2.5px 4.33px 5px 0px rgb( 0, 0, 0 );    }

