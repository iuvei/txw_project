.mail_list  .mail_row .chk { margin-right:15px;  }
.mail_list  .mail_row .title { width:460px;  }
.mail_list  .mail_row .title a { color:#7f7f7f  }
.mail_list  .mail_row .time { margin-left:20px; }
.mail_list  .mail_row.unread,.mail_list  .mail_row.unread .title a  { color:#ff3131; }

