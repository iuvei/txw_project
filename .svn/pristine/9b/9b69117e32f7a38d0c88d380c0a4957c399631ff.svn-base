		<script>
			var $totalva = 0.00;
			var statuslive = 1;
			var statuslobby = 0;
			var statusothers = 0;
			$(document).ready(function() {
				$(".gameKind").click(function() {
					$('.gameKind').removeClass('active');
					$(this).addClass('active');
					if($(this).attr("id") == "live") {
						if(statuslive != 1) {
							statuslive = 1;
						}
						$(".bet-amount-table").hide();
						$("#tablelive").show();
					} else if($(this).attr("id") == "lobby") {
						if(statuslobby != 1) {
							statuslobby = 1;
						}
						$(".bet-amount-table").hide();
						$("#tablelobby").show();

					} else if($(this).attr("id") == "others") {
						if(statusothers != 1) {
							statusothers = 1;
						}
						$(".bet-amount-table").hide();
						$("#tableothers").show();
					}
				});

			});
		</script>
