.slots-list li .jackpot-layer {
  z-index: 3;
  height: 36px;
  line-height: 36px;
  bottom: 0;
  top: auto;
  font-size: 16px;
  text-align: center;
  background-image: -webkit-linear-gradient(left, #66b3ff 0%, #884cff 100%);
  background-image: -o-linear-gradient(left, #66b3ff 0%, #884cff 100%);
  background-image: -webkit-gradient(linear, left top, right top, from(#66b3ff), to(#884cff));
  background-image: linear-gradient(to right, #66b3ff 0%, #884cff 100%);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FF66B3FF', endColorstr='#FF884CFF', GradientType=1);
}

.slots-list li .link-layer {
  z-index: 10;
}

.slots-list li .link-layer > a {
  display: block;
  width: 100%;
  height: 100%;
  opacity: 0;
  filter: alpha(opacity=0);
