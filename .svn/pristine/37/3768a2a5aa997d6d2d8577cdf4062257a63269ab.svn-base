/*提示框*/
.dialog_bg{ display: none; width: 100%; height: 100%; background: #000;filter:alpha(opacity=50); -moz-opacity:0.5; -khtml-opacity: 0.5; opacity: 0.5; position: fixed;top: 0px; left: 0px; z-index: 21;}
.dialog_box{ display: none; width: 640px; height: 244px;background: #fff; border-radius: 8px; position: fixed; top: 325px; left: 0; right: 0; z-index: 22;}
.dialog_box .dialog_title{ width: 630px; text-align: right; margin-right: 10px; margin-top: 10px;}
.dialog_box .dialog_title img{ width: 25px; height: 25px;}
.dialog_box .dialog_content{ width: 550px; padding: 0 40px; text-align: center;}
.dialog_content .msg-desc {
    color: #626262;
    font-size: 14px;
    margin: 0 0 30px;
    line-height: 20px;
}
.dialog_content .msg-info {
    border: 1px solid #dbda96;
    background: #fcfdea;
    color: #614990;
    margin-top: 20px;
    padding: 15px;
    border-radius: 3px;
}

.dialog_content .msg-buttons a {
    display: inline-block;
    min-width: 140px;
    background: #9973e6;
    color: #fff;
    text-align: center;
    border: 1px solid #9973e6;
    border-radius: 3px;
    font-size: 18px;
    letter-spacing: 3px;
    padding: 13px;
    margin: 0 5px;
}

.dialog_content .msg-buttons a.inverted {
    background: #fff;
    color: #9973e6;
    border: 1px solid #9973e6;
}

